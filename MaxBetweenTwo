package Lab1;

public class MaxBetweenTwo {
    public static void main(String[] args) {
        int num1 = 10;
        int num2 = 30;

        // Using ternary operator to find the maximum
        int max = (num1 > num2) ? num1 : num2;

        System.out.println("Maximum Number is: " + max);
    }
}
